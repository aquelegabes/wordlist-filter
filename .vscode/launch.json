{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Script Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "exec",
                "C:/Users/gabes/.dotnet/tools/.store/dotnet-script/1.2.1/dotnet-script/1.2.1/tools/net5.0/any/dotnet-script.dll",
                "${file}",
                // empty space needed
                "","",
                // everything after this is args
                // size
                "--size=5",
                // not contains
                "-n=f,l,i,a",
                // contains
                "-c=o,n,e",
                // position contains
                //"-p=p,3", "-p=e,1", "-p=n,2"
                // position not contains
                // "-o=o,0"
            ],
            "cwd": "${workspaceRoot}",
            "stopAtEntry": false
        }
    ]
}