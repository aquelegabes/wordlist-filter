{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Script Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "exec",
                "C:/Users/gabes/.dotnet/tools/.store/dotnet-script/1.2.1/dotnet-script/1.2.1/tools/net5.0/any/dotnet-script.dll",
                "${file}",
                // empty space needed
                "","",
                // everything after this is args
                // culture/filename
                "--filename=en-us",
                // size
                "--size=5",
                // not contains
                "-n=f,l,i,a",
                // contains
                "-c=o,v",
                // position contains
                "-p=o,0", "-p=u,1",
                // position not contains
                "-o=g,1"
            ],
            "cwd": "${workspaceRoot}",
            "stopAtEntry": false
        }
    ]
}